generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model LogsWebhook {
  id String @id
  alert_name String
  body String

  created_at DateTime @default(now())
}

model LogsOrder {
  id String @id
  alert_name String
  response String
  body String
  message String
  is_error Boolean @default(false)
  
  created_at DateTime @default(now())
}

